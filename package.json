{
  "name": "infersharp-ext",
  "displayName": "InferSharp",
  "description": "VSCode extension experience for InferSharp https://github.com/microsoft/infersharp",
  "author": {
    "name": "Matthew Jin"
  },
  "version": "1.20.0",
  "publisher": "matthew-jin",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:infersharp-ext.analyze"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "infersharp-ext.analyze",
        "title": "InferSharp Analysis",
        "description": "Runs automatic setup if necessary, otherwise, runs Infer#'s analysis on a folder of dlls/pdbs."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionDependencies": [
    "ms-sarifvscode.sarif-viewer"
  ],
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  },
  "license": "SEE LICENSE file",
  "bugs": {
    "url": "https://github.com/matjin/infersharp-extension",
    "email": "matjin@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matjin/infersharp-extension"
  },
  "homepage": "https://github.com/matjin/infersharp"
}
